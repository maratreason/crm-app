{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "quotes": "off",
        "arrow-parens": "off",
        "linebreak-style": "off",
        "indent": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off",
        "embedded-language-formatting": "off"
    },
    "extends": ["google"]
}